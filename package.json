{
  "name": "@kushki/js-sdk",
  "version": "0.0.1",
  "scripts": {
    "bootstrap": "yarn install && npm run tsc:interface && yarn build:npm && cd ./showcase && yarn install && yarn dev",
    "dev": "cd ./showcase && vite --c vite.config.ts",
    "build:npm": "tsc && vite build --c vite.config-npm.ts && yarn cp:package",
    "build:cdn": "npx webpack --config webpack-cdn.config.js",
    "build": "npm run validate && npm run build:npm && npm run build:cdn",
    "lint": "npm run format && npm run lint:fix && eslint src/**/*.ts",
    "preview": "vite preview --c vite.config-npm.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "format": "prettier --write src/**/*.ts --config ./.prettierrc",
    "prepare": "husky install",
    "tsc:interface": "cd json2ts/bin && node kushki json2ts ",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test": "npm run test:unit && npm run test:coverage",
    "validate": "npm run lint && npm run test",
    "cp:package": "cp ./package.json dist",
    "publish:alpha": "npm run build && npm publish --tag alpha",
    "publish:latest": "npm run build && npm publish ./dist"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./Kushki.js"
    },
    "./Payment": {
      "import": "./module/Payment/Payment.js"
    }
  },
  "engines": {
    "node": ">=18.12.1",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "dependencies": {
    "@krakenjs/zoid": "^10.3.1",
    "@kushki/cardinal-sandbox-js": "^1.0.4",
    "axios": "^1.5.0",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/axios": "^0.14.0",
    "@types/inversify": "^2.0.33",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.1",
    "@types/sinon": "^10.0.16",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "path": "^0.12.7",
    "prettier": "3.0.1",
    "ts-jest": "^29.1.1",
    "ts-mockery": "^1.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.2",
    "vite-tsconfig-paths": "^4.2.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
