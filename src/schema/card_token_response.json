{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "CardTokenResponse",
  "type": "object",
  "additionalProperties": false,
  "definitions": {
    "CardInfo": {
      "type": "object",
      "properties": {
        "expirationDate": {
          "type": "string"
        },
        "bin": {
          "type": "string"
        },
        "lastFourDigits": {
          "type": "string"
        },
        "brand": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "required": ["expirationDate", "bin", "lastFourDigits", "brand"]
    }
  },
  "properties": {
    "token": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{32}$"
    },
    "secureService": {
      "type": "string"
    },
    "secureId": {
      "type": "string"
    },
    "settlement": {
      "type": "number"
    },
    "security": {
      "type": "object",
      "properties": {
        "authRequired": {
          "type": "boolean"
        },
        "acsURL": {
          "type": "string"
        },
        "authenticationTransactionId": {
          "type": "string"
        },
        "paReq": {
          "type": "string"
        }
      }
    },
    "cardInfo": {
      "$ref": "#/definitions/CardInfo"
    }
  },
  "required": ["token"]
}
